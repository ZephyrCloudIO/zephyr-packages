    "@nx/react": "20.6.2",
    "@nx/rollup": "20.6.2",
    "@nx/rspack": "20.6.2",
    "@nx/web": "20.6.2",
    "@nx/webpack": "20.6.2",
    "@nx/workspace": "20.6.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@rollup/plugin-url": "8.0.2",
    "@rspack/cli": "^1.2.8",
    "@rspack/core": "1.2.8",
    "@rspack/plugin-react-refresh": "^1.0.1",
