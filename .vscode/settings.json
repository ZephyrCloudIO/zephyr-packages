{
  "biome.enabled": false,
  "files.associations": {
    "wrangler.json": "jsonc"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 300,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.removeUnusedImports": "explicit",
    "source.groupImports": "explicit"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": ["fastly"],
  "nxConsole.generateAiAgentRules": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#b450f3",
    "activityBar.background": "#b450f3",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f3b24c",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#b450f3",
    "statusBar.background": "#a020f0",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#b450f3",
    "statusBarItem.remoteBackground": "#a020f0",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#a020f0",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#a020f099",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#a020f0"
}
