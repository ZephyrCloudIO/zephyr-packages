{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "tui": {
    "autoExit": 1,
    "enabled": false
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build",
        "projects"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "@nx/jest:jest": {
      "cache": true,
      "inputs": [
        "default",
        "^production",
        "{workspaceRoot}/jest.preset.js"
      ],
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore",
        "{workspaceRoot}/eslint.config.js"
      ]
    },
    "@nx/js:tsc": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "@nx/rollup:rollup": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "@nx/esbuild:esbuild": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "e2e": {
      "cache": true,
      "inputs": [
        "default",
        "^production"
      ]
    },
    "@nx/webpack:webpack": {
      "cache": true,
      "inputs": [
        "production",
        "^production",
        {
          "env": "NX_MF_DEV_REMOTES"
        }
      ],
      "dependsOn": [
        "^build"
      ]
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/test-setup.[jt]s",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/eslint.config.js"
    ],
    "sharedGlobals": []
  },
  "generators": {
    "@nx/web:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nx/react": {
      "application": {
        "babel": true,
        "style": "css",
        "linter": "eslint",
        "bundler": "webpack"
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "useInferencePlugins": false,
  "defaultBase": "master",
  "release": {
    "projects": [
      "libs/*"
    ],
    "changelog": {
      "workspaceChangelog": {
        "createRelease": "github",
        "file": false
      }
    },
    "version": {
      "preVersionCommand": "npx nx run-many -t build -p libs/*",
      "git": {
        "commit": false,
        "stageChanges": false,
        "tag": false
      },
      "currentVersionResolver": "registry",
      "preserveLocalDependencyProtocols": false,
      "versionActionsOptions": {
        "skipLockFileUpdate": true
      }
    }
  },
  "plugins": [
    {
      "plugin": "@nx/rspack/plugin",
      "include": ["examples/rspack-nx-mf/**"],
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "buildDepsTargetName": "build-deps",
        "watchDepsTargetName": "watch-deps"
      }
    }
  ],
  "nxCloudAccessToken": "Zjg0MzllZTQtYTE2Mi00MWMwLWE2ZDUtOTE5ZWI5OGJkYTM5fHJlYWQtd3JpdGU="
}
