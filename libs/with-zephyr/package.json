{
  "name": "with-zephyr",
  "version": "0.1.13",
  "description": "A codemod to automatically add withZephyr plugin to bundler configurations",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZephyrCloudIO/zephyr-packages.git",
    "directory": "libs/with-zephyr"
  },
  "type": "module",
  "author": {
    "name": "ZephyrCloudIO",
    "url": "https://github.com/ZephyrCloudIO"
  },
  "license": "Apache-2.0",
  "bin": {
    "with-zephyr": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "start": "node dist/index.js",
    "test": "rstest run",
    "test:watch": "rstest --watch",
    "test:coverage": "rstest --coverage",
    "typecheck": "tsc --noEmit",
    "patch-version": "pnpm version"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "glob": "^13.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.28.4",
    "@rstest/core": "^0.8.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^22.13.13",
    "@rslib/core": "^0.19.0",
    "typescript": "^5.9.2",
    "zephyr-rsbuild-plugin": "workspace:*"
  },
  "keywords": [
    "zephyr",
    "codemod",
    "bundler",
    "webpack",
    "vite",
    "rollup",
    "rspack",
    "rsbuild",
    "rslib",
    "astro",
    "modernjs",
    "rolldown",
    "rspress"
  ],
  "packageManager": "pnpm@10.30.3"
}
